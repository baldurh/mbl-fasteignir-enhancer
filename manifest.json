{
  "name": "Fasteignaleit - viðbætur",
  "short_name": "Fasteignir++",
  "version": "2.1.3",
  "manifest_version": 2,
  "description": "Bætir upplifun á fasteignaleit mbl.is og fasteignir.is",
  "author": "Baldur Már Helgason",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.mbl.is/fasteignir/*",
        "http://fasteignir.visir.is/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/lodash.min.js",
        "src/common.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://www.mbl.is/fasteignir/",
        "http://www.mbl.is/fasteignir/leit/*"
      ],
      "js": [
        "src/mbl/search-form.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://www.mbl.is/fasteignir/leit/*",
        "http://www.mbl.is/fasteignir/opinhus/"
      ],
      "js": [
        "src/mbl/leit.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://www.mbl.is/fasteignir/fasteign/*"
      ],
      "js": [
        "src/mbl/fasteign.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://www.mbl.is/fasteignir/kortaleit/*"
      ],
      "js": [
        "src/mbl/kort.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://fasteignir.visir.is/"
      ],
      "js": [
        "src/visir/search-form-edit.js",
        "src/visir/search-form.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://fasteignir.visir.is/property/*"
      ],
      "js": [
        "src/visir/property.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://fasteignir.visir.is/search/*"
      ],
      "js": [
        "src/visir/search-form-edit.js",
        "src/visir/search.js"
      ],
      "run_at": "document_end"
    }
  ]
}
